(cd ~ && git clone https://github.com/mr-youbella/tt C_Craft) && echo "source ~/C_Craft/Processing/script/alias.sh" >> ~/.bashrc && echo "source ~/C_Craft/Processing/script/alias.sh" >> ~/.zshrc && echo "alias upc='(cd ~/C_Craft && git pull) && (cd ~/C_Craft/Processing && make upc) && source ~/.bashrc && source ~/.zshrc'" >> ~/.bashrc && echo "alias upc='(cd ~/C_Craft && git pull) && (cd ~/C_Craft/Processing && make upc) && source ~/.bashrc && source ~/.zshrc'" >> ~/.zshrc && source ~/.bashrc && source ~/.zshrc && exec bash && start
